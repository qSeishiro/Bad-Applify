{
  "manifest_version": 3,
  "name": "BadApplify Stream",
  "version": "2.1",
  "description": "Plays Bad Apple on YouTube from URL",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}