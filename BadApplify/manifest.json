{
  "manifest_version": 3,
  "name": "BadApplify Debug",
  "version": "2.0",
  "description": "Plays Bad Apple on YouTube pages",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["bad_apple.mp4"],
      "matches": ["<all_urls>"]
    }
  ]
}